{
  "name": "uiucbus",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "1.2.18",
    "@fortawesome/free-solid-svg-icons": "5.8.2",
    "@fortawesome/react-fontawesome": "0.1.4",
    "@material-ui/core": "3.9.3",
    "@sentry/browser": "5.4.0",
    "axios": "0.18.1",
    "bootstrap": "4.3.1",
    "date-fns": "2.0.0-alpha.27",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-prettier": "4.3.0",
    "lint-staged": "8.1.7",
    "node-sass": "4.12.0",
    "prettier": "1.17.1",
    "pretty-quick": "1.11.0",
    "prop-types": "15.7.2",
    "pulltorefreshjs": "0.1.17",
    "react": "16.8.6",
    "react-autosuggest": "9.4.3",
    "react-cookie": "3.1.2",
    "react-dom": "16.8.6",
    "react-ga": "2.5.7",
    "react-geolocated": "2.5.0",
    "react-pose": "4.0.8",
    "react-router-dom": "5.0.0",
    "react-scripts": "3.0.1",
    "reactstrap": "7.1.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "lint-all": "eslint 'src/**/*.{jsx,js}'",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "prettier-all": "prettier --write src/**/*.js",
    "prettier-check": "prettier -c src/**/*.{js,json,css,scss,md}",
    "eject": "react-scripts eject",
    "make-cname": "cd build && echo uiucbus.com > CNAME",
    "make-404": "cp 404.html build/404.html",
    "http-server": "$(npm bin)/http-server ./build --silent -p 3000"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "homepage": "http://uiucbus.com",
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": "eslint",
    "*.{js,json,scss,md,jsx}": [
      "prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
    "coveralls": "3.0.3",
    "enzyme": "3.9.0",
    "enzyme-adapter-react-16": "1.13.2",
    "enzyme-to-json": "3.3.5",
    "eslint-plugin-cypress": "2.2.1",
    "http-server": "0.11.1",
    "husky": "1.3.1"
  }
}
