    "http-server": "$(npm bin)/http-server ./build --silent -p 3000"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "homepage": "http://uiucbus.com",
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": "eslint",
    "*.{js,json,scss,md,jsx}": [
      "prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
    "coveralls": "^3.0.11",
